#panel-initial.container
    .row.d-flex.justify-content-center
        .col-4.d-flex.justify-content-evenly
            button#dislike.btn.btn-circle(name="dislike", onclick="dislike()")
                i.bi.bi-hand-thumbs-down-fill(style="color: #c44141")
            button#end.btn.btn-circle(name="end", onclick="end()")
                i.bi.bi-check-circle-fill(style="color: #41945f")
            button#like.btn.btn-circle(name="like", onclick="like()")
                i.bi.bi-hand-thumbs-up-fill(style="color: #2b95ff")
